<h1>Contacts <%= @contacts.count %></h1>
<table>
  <tr>
    <th>id</th>
    <th>name</th>
    <th>email</th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><%= contact.id %></td>
      <td><%= contact.first_name %>&nbsp;<%= contact.last_name %></td>
      <td><%= contact.email_address %></td>
    </tr>
  <% end %>
</table>